# ============================================
# Grand Prix Realty - Buyer Search App
# Environment Variables
# ============================================

# Trestle API Configuration
TRESTLE_CLIENT_ID=your_client_id_here
TRESTLE_CLIENT_SECRET=your_client_secret_here
TRESTLE_TOKEN_URL=https://api.trestle.io/oauth/token
TRESTLE_ODATA_URL=https://api.trestle.io/odata/Property

# Supabase - Browser Client (public, can be exposed)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase - Server Side (private - DO NOT expose to browser)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
