# Production Environment Variables for GPR Stack
# Copy this to .env on the server

# Database - PostgreSQL with PostGIS on Supabase
DATABASE_URL=postgresql+asyncpg://postgres.supabase:YOUR_PASSWORD@localhost:5432/postgres

# API Configuration
ENVIRONMENT=production
DEBUG=false

# CORS Origins
CORS_ORIGINS=https://grandprixrealty.agency,https://www.grandprixrealty.agency

# Google Maps API (for geocoding)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Trestle MLS API (for buyer-search-app)
TRESTLE_CLIENT_ID=your_trestle_client_id
TRESTLE_CLIENT_SECRET=your_trestle_client_secret
TRESTLE_TOKEN_URL=https://api.cotality.com/trestle/oidc/connect/token
TRESTLE_ODATA_URL=https://api.cotality.com/trestle/odata/Property

# Supabase (for SEO Lab API - service role key)
SUPABASE_URL=https://supabase.grandprixrealty.agency
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
